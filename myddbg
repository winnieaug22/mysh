#!/bin/bash
source mycolor
myddbg_dir="/home/winnie/mysh/myddbgdir/"
echo -e "$yellow--- myddbg.h ---$end"
cat ${myddbg_dir}myddbg.h
echo -e "$yellow--- tt.c ---$end"
cat ${myddbg_dir}tt.c
echo -e "$yellow--- Makefile ---$end"
cat ${myddbg_dir}Makefile
echo -e "$yellow--- Makefile_df ---$end"
echo -e "$red"
cat ${myddbg_dir}Makefile_df
echo -e "$end"

